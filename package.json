{
    "name": "node-react-ui",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">= 12.13.0"
    },
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Edge versions"
    ],
    "pkg": {
        "assets": "target/renderer/*"
    },
    "bin": "target/main/main.js",
    "scripts": {
        "prettier:fix": "prettier --write \"{src,test}/**/*.{ts,tsx,json,css,html}\"",
        "prettier:check": "prettier --check \"{src,test}/**/*.{ts,tsx,json,css,html}\"",
        "typescript": "tsc --noEmit -p .",
        "test": "yarn run prettier:check && yarn run typescript",
        "build": "cross-env TS_NODE_FILES=true node -r ts-node/register/transpile-only src/build/build.ts",
        "start:renderer": "parcel serve src/renderer/index.html --out-dir target/serve --port 8080 --no-autoinstall",
        "start:main": "cross-env NODE_ENV=development node -r ts-node/register/transpile-only src/main/main.ts"
    },
    "dependencies": {
        "better-sqlite3": "7.0.0",
        "puppeteer-core": "2.1.1",
        "react": "16.13.0",
        "react-dom": "16.13.0",
        "react-router-dom": "5.1.2",
        "winstrap": "0.5.12"
    },
    "devDependencies": {
        "@types/better-sqlite3": "5.4.0",
        "@types/minimist": "1.2.0",
        "@types/node": "13.7.6",
        "@types/puppeteer-core": "2.0.0",
        "@types/react": "16.9.23",
        "@types/react-dom": "16.9.5",
        "@types/react-router-dom": "5.1.3",
        "@types/rimraf": "3.0.0",
        "@zeit/ncc": "0.22.1",
        "cross-env": "7.0.2",
        "nexe": "3.3.2",
        "parcel": "1.12.4",
        "pkg": "4.4.3",
        "prettier": "2.0.2",
        "rimraf": "3.0.2",
        "ts-node": "8.6.2",
        "typescript": "3.8.2"
    }
}
