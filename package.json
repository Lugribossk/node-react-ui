{
    "name": "node-react-ui",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">= 10.13.0"
    },
    "scripts": {
        "prettier:fix": "prettier --write \"{src,test}/**/*.{ts,tsx,json}\"",
        "prettier:check": "prettier --list-different \"{src,test}/**/*.{ts,tsx,json}\"",
        "prompt:js": "parcel build src/prompt/prompt-app.tsx --out-dir target --target node --bundle-node-modules --no-source-maps",
        "prompt:executable": "nexe target/prompt-app.js -o target/prompt-app.exe -r node_modules/winstrap/dist/css/winstrap.min.css",
        "fixsubsystem": "editbin /subsystem:windows target/prompt-app.exe"
    },
    "dependencies": {
        "carlo": "0.9.28",
        "nexe": "2.0.0-rc.34",
        "parcel": "1.10.3",
        "prettier": "1.15.2",
        "puppeteer-core": "1.10.0",
        "purecss": "1.0.0",
        "react": "16.6.3",
        "react-dom": "16.6.3",
        "ts-node": "7.0.1",
        "typescript": "3.1.6",
        "winstrap": "0.5.12"
    },
    "devDependencies": {
        "@types/node": "10.12.9",
        "@types/prettier": "1.15.0",
        "@types/puppeteer-core": "1.9.0",
        "@types/react": "16.7.6",
        "@types/react-dom": "16.0.9"
    }
}
